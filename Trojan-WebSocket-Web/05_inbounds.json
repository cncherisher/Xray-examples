{
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 443,
      "protocol": "trojan",
      "settings": {
        "clients": [
          {
            "password": "password"
          }
        ],
        "fallbacks": [
          {
            "dest": 80
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "rejectUnknownSni": true,
          "minVersion": "1.2",
          "certificates": [
            {
              "certificateFile": "/usr/local/etc/xray/ssl/cert.pem",
              "keyFile": "/usr/local/etc/xray/ssl/privkey.pem"
            }
          ]
        },
        "wsSettings": {
          "acceptProxyProtocol": false,
          "path": "/wsapi"
        }
      }
    }
  ]
}
